%script#body_portfolio-tab_template{type: 'text/template'}

%script#body_portfolio-tab-item_template{type: 'text/template'}
  .card-item.row
    .col.s12
      .floating-action.like.pointer

      .namepic
        %img.rounded-border{src: ''}
      .description
        %span.entry-name
          %a{href: '<%- public_link %>'.html_safe}
            <%- entry_name %>
        <%- description_string %>

